{
    "name": "greyscript-meta",
    "version": "2.4.2",
    "description": "Meta description for GreyScript API",
    "main": "dist/index",
    "typings": "dist/index",
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "npm run build:local && npm run build:web && npm run move:web",
        "build:web": "rollup --config rollup.config.js --bundleConfigAsCjs",
        "build:local": "npm run clean && tsc -p .",
        "move:web": "cp -a web/. dist/",
        "watch": "tsc -w -p .",
        "clean": "rm -rf dist",
        "lint": "eslint ./src/**/*.ts",
        "lint:fix": "eslint --fix ./src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ayecue/greyscript-meta.git"
    },
    "author": {
        "name": "ayecue",
        "email": "soerenwehmeier@googlemail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ayecue/greyscript-meta/issues"
    },
    "homepage": "https://github.com/ayecue/greyscript-meta#readme",
    "devDependencies": {
        "@babel/core": "^7.16.10",
        "@babel/helpers": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.16.8",
        "@babel/preset-env": "^7.16.8",
        "@babel/preset-react": "^7.18.6",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/jest": "^27.0.3",
        "@types/memoizee": "^0.4.8",
        "@types/node": "^17.0.0",
        "@types/prismjs": "^1.26.0",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "jest": "^27.4.5",
        "netlify-cli": "^11.3.0",
        "nodemon": "^2.0.15",
        "prismjs": "^1.29.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.7",
        "rollup": "^3.20.2",
        "rollup-plugin-external-globals": "^0.7.3",
        "rollup-plugin-node-globals": "^1.0.1",
        "rollup-plugin-polyfill-node": "^0.12.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "keywords": [
        "greyscript",
        "greyhack"
    ],
    "dependencies": {
        "meta-utils": "~1.0.0",
        "react-in-viewport": "^1.0.0-alpha.30"
    }
}
