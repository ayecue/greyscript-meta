      "type": "function",
      "arguments": [
        {
          "label": "ip",
          "type": "string",
          "opt": false
        }
      ],
      "returns": [
        "string",
        "number"
      ]
    },
    "scp": {
      "type": "function",
      "arguments": [
        {
          "label": "file",
          "type": "string"
        },
        {
          "label": "folder",
          "type": "string"
        },
        {
          "label": "remoteShell",
          "type": "shell"
